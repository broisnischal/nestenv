#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nees/nestenv/node_modules/.pnpm/bunchee@5.5.1_typescript@5.6.2/node_modules/bunchee/dist/bin/node_modules:/Users/nees/nestenv/node_modules/.pnpm/bunchee@5.5.1_typescript@5.6.2/node_modules/bunchee/dist/node_modules:/Users/nees/nestenv/node_modules/.pnpm/bunchee@5.5.1_typescript@5.6.2/node_modules/bunchee/node_modules:/Users/nees/nestenv/node_modules/.pnpm/bunchee@5.5.1_typescript@5.6.2/node_modules:/Users/nees/nestenv/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nees/nestenv/node_modules/.pnpm/bunchee@5.5.1_typescript@5.6.2/node_modules/bunchee/dist/bin/node_modules:/Users/nees/nestenv/node_modules/.pnpm/bunchee@5.5.1_typescript@5.6.2/node_modules/bunchee/dist/node_modules:/Users/nees/nestenv/node_modules/.pnpm/bunchee@5.5.1_typescript@5.6.2/node_modules/bunchee/node_modules:/Users/nees/nestenv/node_modules/.pnpm/bunchee@5.5.1_typescript@5.6.2/node_modules:/Users/nees/nestenv/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/cli.js" "$@"
else
  exec node  "$basedir/../../dist/bin/cli.js" "$@"
fi
